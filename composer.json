{
    "name": "takaram/uuid47",
    "description": "PHP implementation of UUIDv47",
    "type": "library",
    "require": {
        "php": "^8.2",
        "ext-sodium": "*",
        "ramsey/uuid": "^4.5"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^12.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Takaram\\Uuid47\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Takaram\\Uuid47\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Takuya Aramaki",
            "email": "takaram71@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
